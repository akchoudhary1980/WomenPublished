@{   
    Layout = "~/Views/Shared/_Admin.cshtml";
}
    <div class="page-header animated bounceInRight">
        <div class="card">
            <div class="card-block bg-primary">
                <div class="row ">
                    <div class="col-lg-4">
                        <h5 class="text-uppercase font-weight-bold">Country List</h5>
                    </div>
                    <div class="col-lg-8 align-items-end">
                        <div class="page-header-breadcrumb">
                            <ul class="breadcrumb-title">
                                <li class="breadcrumb-item">
                                    <a href="@Url.Action("Index", "Dashboard")"> <i class="icofont icofont-home" style="color:white"></i> </a>
                                </li>
                                <li class="breadcrumb-item">
                                    <a href="@Url.Action("Index", "Country")" style="color:white">Home / Masters / Country List</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
<div class="row animated bounceInDown">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <div class="row">
                    <div class="col-md-9 col-9"><h3>@ViewData["PageTitle"]</h3></div>
                    <div class="col-md-3 col-3">
                        <a href="@Url.Action("Create", "Country")" class="btn btn-primary btn-outline-primary float-right">
                            <i class="icofont icofont-user-alt-3"></i> Add
                        </a>
                    </div>
                </div>
            </div>
            <div class="card-block">
                @Wom.DataLib.QueryDataTableToHTMLTableWithButton("Select CountryModels.CountryID as '#', CountryModels.CountryName as 'Country Name', Region from CountryModels", "Country")
            </div>
        </div>       
    </div>
</div>